{
  "expo": {
    "name": "VisGO",
    "slug": "VisGO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Go_white.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Go_white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Go_White.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/Go_white.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(VisGo) to access your camera",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
