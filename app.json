{
  "expo": {
    "name": "GoVis",
    "slug": "GoVis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Go_white.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Go_white.png",
      "resizeMode": "contain",
      "backgroundColor": "#0297b7"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Go_white.png",
        "backgroundColor": "#0297b7"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.VisGO"
    },
    "web": {
      "favicon": "./assets/Go_white.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(GoVis) to access your camera",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a52a1c92-4262-48c3-9d5a-5c0f8f350b66"
      }
    },
    "owner": "wesleynederpel",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a52a1c92-4262-48c3-9d5a-5c0f8f350b66"
    }
  }
}
